\begin_inset Text

\begin_layout Plain Layout

\lang british
OPTIM:alg [DIR=d] [START=start] [STOP=stop] [STEP=step] [MAXSTEP=maxstep]
 [SAVEVAR=savefile] [LOADVAR=loadfile] [TOLL=toll] [F
